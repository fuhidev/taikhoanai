{
 "manifest_version": 3,
 "name": "AI Access Manager",
 "version": "1.0.0",
 "description": "Extension để truy cập các nền tảng AI thông qua cookie",
 "permissions": ["storage", "activeTab", "cookies", "scripting"],
 "host_permissions": [
  "https://chat.openai.com/*",
  "https://gemini.google.com/*",
  "https://leonardo.ai/*",
  "https://hailuoai.com/*",
  "http://localhost:3000/*"
 ],
 "background": {
  "service_worker": "background.js"
 },
 "content_scripts": [
  {
   "matches": [
    "https://chat.openai.com/*",
    "https://gemini.google.com/*",
    "https://leonardo.ai/*",
    "https://hailuoai.com/*"
   ],
   "js": ["content.js"],
   "run_at": "document_start"
  }
 ],
 "action": {
  "default_popup": "popup.html",
  "default_title": "AI Access Manager"
 },
 "web_accessible_resources": [
  {
   "resources": ["inject.js"],
   "matches": ["<all_urls>"]
  }
 ]
}
